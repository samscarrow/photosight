# PhotoSight Environment Variables Example
# Copy to .env and fill in your actual values

# Database Configuration
PHOTOSIGHT_DATABASE_URL=postgresql://user:password@host:port/database?sslmode=require

# MCP Server Configuration
PHOTOSIGHT_MCP_PASSWORD=your_readonly_password_here

# Privacy Settings
ENABLE_TELEMETRY=false
STORE_TEMP_FILES=false
MAX_IMAGE_SIZE_MB=10

# Development Settings
DEBUG=false
LOG_LEVEL=info